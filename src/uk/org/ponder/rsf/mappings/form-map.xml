<?xml version="1.0"?>
<mapperentry targetclass="uk.org.ponder.rsf.components.UIForm">
  <taghandler xmlname="id" xml-form="attribute">
    <fieldname>ID</fieldname>
  </taghandler>
  <taghandler xmlname="component*">
    <setmethod>addComponent</setmethod>
    <getmethod>flattenChildren</getmethod>
  </taghandler>
  <taghandler xmlname="parameter*">
    <fieldname>parameters</fieldname>
    <javaclass>uk.org.ponder.rsf.components.UIParameter</javaclass>
  </taghandler>  
  <defaultible>true</defaultible>
</mapperentry>